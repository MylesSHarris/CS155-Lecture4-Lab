<Window x:Class="Part1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Part1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="speedText" HorizontalAlignment="Left" Margin="366,154,0,0" Text="Speed" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="speedLimitText" HorizontalAlignment="Left" Margin="366,185,0,0" Text="Speed Limit" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button Content="Button" HorizontalAlignment="Left" Margin="366,217,0,0" VerticalAlignment="Top" Height="30" Width="120" Click="Button_Click"/>
        <Canvas Margin="366,75,311,294">
            <Canvas x:Name="statusCanvas" Height="22" Width="123" Canvas.Top="33"/>
        </Canvas>
        <TextBlock x:Name="statusText" HorizontalAlignment="Left" Margin="366,112,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Height="18" Width="120"/>
    </Grid>
</Window>
